pocet vyskytu:=COUNTX(FILTER(data;data[Pocet obyvatel] <= MAX([Hranice]));data[Pocet obyvatel])




polsko max rok a predchozi rok:=IF(
MAXX(FILTER(data;data[Zeme]="Polsko"&&data[Rok] = MAX(data[Rok]));data[Pocet obyvatel])
<
MAXX(FILTER(data;data[Zeme]="Polsko"&&data[Rok] = MAX(data[Rok])-1);data[Pocet obyvatel]);"pokles";"nárůst")



zeme min populace max rok:=CALCULATE(
FIRSTNONBLANK(data[Zeme];data[Zeme]);FILTER(data;data[Pocet obyvatel] =
MINX(FILTER(data;data[Rok] = MAX(data[Rok]));data[Pocet obyvatel])))




prumer vsechny roky zeme:=AVERAGEX(
FILTER(ALL(data);data[Zeme] = VALUES(data[Zeme]));data[Pocet obyvatel])
