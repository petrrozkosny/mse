pocet vyskytu:=COUNTX(FILTER(data;data[Pocet obyvatel] <= MAX([Hranice]));data[Pocet obyvatel])




polsko max rok a predchozi rok:=IF(
MAXX(FILTER(data;data[Zeme]="Polsko"&&data[Rok] = MAX(data[Rok]));data[Pocet obyvatel])
<
MAXX(FILTER(data;data[Zeme]="Polsko"&&data[Rok] = MAX(data[Rok])-1);data[Pocet obyvatel]);"pokles";"nárůst")



zeme min populace max rok:=CALCULATE(
FIRSTNONBLANK(data[Zeme];data[Zeme]);FILTER(data;data[Pocet obyvatel] =
MINX(FILTER(data;data[Rok] = MAX(data[Rok]));data[Pocet obyvatel])))




prumer vsechny roky zeme:=AVERAGEX(
FILTER(ALL(data);data[Zeme] = VALUES(data[Zeme]));data[Pocet obyvatel])




porovnani populace slicer:=SUM(data[Pocet obyvatel])
-
SUMX(
FILTER(ALL(data);data[Rok] = MAX(data[Rok]) - MAX('pocet let zpet'[pocet let zpet]));data[Pocet obyvatel])



srazky mezirocne:=IFERROR(
SUM(data[PRCP])
-
SUMX(FILTER(ALL(data);data[Rok] 

= MAX(data[Rok])-1
&&data[NAME] = VALUES(data[NAME])

);data[PRCP]);BLANK())




srazky kumulativne:=SUMX(
FILTER(
ALL(data);
data[Rok] = MAX(data[Rok])
&&data[NAME] = VALUES(data[NAME])
&&data[Mesic] <=MAX(data[Mesic]));data[PRCP])




srazky minule obdobi:=IFERROR(SUMX(
FILTER(ALL(data);
data[Rok]= MAX(data[Rok]) - MAX('pocet let zpet'[pocet let zpet])
&&data[NAME] = VALUES(data[NAME]));data[PRCP]);BLANK())




=CUBEVALUE("ThisWorkbookDataModel";"[Measures].[min_srazky]";"[data].[Rok].[All].["&A51&"]")


rok max srazek:=MAXX(FILTER('dim roky';'dim roky'[Suma srazek]= max('dim roky'[Suma srazek]));'dim roky'[Rok])


prumer dennich srazek max rok:=AVERAGEX(FILTER(data;data[Rok]=[rok max srazek]);data[PRCP])



=
CALCULATE(
FIRSTNONBLANK(data[NAME];data[NAME]);
FILTER(data;data[PRCP] = EARLIER([Maximalni srazky v roce])
&&data[Rok] = EARLIER('dim roky'[Rok])))



=MAXX(FILTER(data;data[PRCP]=EARLIER('dim roky'[Maximalni srazky v roce])&&data[Rok] = EARLIER('dim roky'[Rok]));data[DATE])
